backdrop on
sync on
sync rate 60

set text size 20

color fog rgb(28, 73, 140)
set camera range 10, 48000
fog distance 48000

load agm level "main.amp"
set bba 3
set bbb 15

setmaindir

runiscript
agm distance load
update delta

set mouse visible 0

do
	update delta

	agm distance load
	
	runlscript
	
	sync
loop

end